{
  "name": "skygame-data",
  "version": "0.0.6",
  "description": "Data repository for SkyGame-Planner.",
  "scripts": {
    "json-build": "node ./src/scripts/json-build.cjs",
    "build": "npm run json-build && tsc",
    "test": "tsx tests/try-parse.ts"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./interfaces/*": {
      "types": "./dist/interfaces/*.d.ts"
    }
  },
  "files": [
    "dist",
    "assets"
  ],
  "author": "Silverfeelin",
  "license": "MIT",
  "homepage": "https://github.com/Silverfeelin/SkyGame-Data#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Silverfeelin/SkyGame-Data.git"
  },
  "bugs": {
    "url": "https://github.com/Silverfeelin/SkyGame-Data/issues"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "jsonc-parser": "^3.3.1",
    "luxon": "^3.7.2"
  }
}
